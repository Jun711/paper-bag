{
  "name": "paperbag",
  "version": "1.0.0",
  "description": "shopping list desktop app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . 'Paper Bag' --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --out=build",
    "package-win": "electron-packager . 'Paper Bag' --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --out=build --version-string.CompanyName=Name --version-string.FileDescription=Desc --version-string.ProductName=\"Paper Bag\"",
    "package-linux": "electron-packager . 'Paper Bag' --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --out=build"
  },
  "author": "Jun",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^14.0.4"
  }
}
