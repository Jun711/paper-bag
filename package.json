{
  "name": "paperbag",
  "version": "1.0.0",
  "description": "shopping list desktop app",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . 'Paper Bag' --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . 'Paper Bag' --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Paper Bag\"",
    "package-linux": "electron-packager . 'Paper Bag' --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "author": "Jun Goh",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "electron-packager": "^14.0.3",
    "electron": "^6.0.0"
  }
}
